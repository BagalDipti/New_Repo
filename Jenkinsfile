pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello Everyone!!'
            }
        }
         stage('Build') {
            steps {
                echo 'Building'
            }
        }
         stage('deploy') {
            steps {
                echo 'Deploying'
            }
        }
         stage('Test') {
            steps {
                echo 'Testing'
            }
        }
         stage('Release') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
